{
    "manifest_version": 3,
    "name": "My React Chrome Extension",
    "version": "1.0",
    "description": "A Chrome extension built with React",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": [
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }